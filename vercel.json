{
  "buildCommand": "cd proyectoFinal && npm install && npm run build",
  "outputDirectory": "proyectoFinal/dist/proyecto-final/browser",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/backend/server.js"
    },
    {
      "source": "/uploads/(.*)",
      "destination": "/backend/server.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
